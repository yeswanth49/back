{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "openai.service.js",
          "assistant.json",
          "Sample.docx"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/start",
      "dest": "/index.js"
    },
    {
      "src": "/chat",
      "dest": "/index.js"
    }
  ],
  "env": {
    "OPENAI_API_KEY": "@openai_api_key"
  }
}
